Deploy to Cloud Run:
  gcloud run deploy ledgerx-api `
    --source . `
    --region us-central1 `
    --project ledgerx-mlops `
    --set-env-vars GOOGLE_CLOUD_PROJECT=ledgerx-mlops `
    --set-env-vars LOG_LEVEL=INFO `
    --allow-unauthenticated

Get Service URL:
  $URL = gcloud run services describe ledgerx-api `
    --region us-central1 `
    --project ledgerx-mlops `
    --format='value(status.url)'
  Write-Host $URL

